#jinja2:variable_start_string:'[%' , variable_end_string:'%]', trim_blocks: False
#!/usr/bin/bash
cat << EOF | tee -a apache_basic/install_apache.yml

  tasks:
    - name: install apache
      yum:
        name: httpd
        state: present
  
    - name: start httpd
      service:
        name: httpd
        state: started
EOF
