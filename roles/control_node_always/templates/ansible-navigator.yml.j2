---
ansible-navigator:
  ansible:
    inventory:
      entries:
      - /home/{{ username }}/lab_inventory/hosts

  execution-environment:
    image: {{ lookup('vars', workshop_type + '_ee') }}
    enabled: true
    container-engine: podman
    pull:
      policy: missing
    volume-mounts:
    - src: "/etc/ansible/"
      dest: "/etc/ansible/"
