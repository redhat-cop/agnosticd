apiVersion: operators.coreos.com/v1alpha1
kind: Subscription
metadata:
  name: amq-online
  namespace: '{{ operators_project }}'
spec:
  channel: stable
  installPlanApproval: Manual
  name: amq-online
  source: redhat-operators-snapshot-dil
  sourceNamespace: {{ snapshot_operator_project }}
  startingCSV: amq-online.{{ csv_version }}
  config:
    env:
    - name: ENMASSE_OPENSHIFT
      value: "true"
