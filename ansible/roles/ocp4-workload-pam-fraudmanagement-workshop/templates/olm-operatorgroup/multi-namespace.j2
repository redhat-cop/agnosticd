---
kind: List
apiVersion: v1
metadata:
  name: olm-operatorgroup
items:
- apiVersion: operators.coreos.com/v1
  kind: OperatorGroup
  metadata:
    name: {{ _namespace }}
    namespace: {{ _namespace }}
  spec:
    targetNamespaces:
    {% for current in namespace_nums %}
      - {{ namespace_prefix }}{{ current }}
    {% endfor %}