apiVersion: route.openshift.io/v1
kind: Route
metadata:
  labels:
    app: "{{ _haproxy_name }}"
  name: "{{ _haproxy_name }}"
  namespace: "{{ _haproxy_project }}"
spec: 
  port:
    targetPort: 8080
  subdomain: ""
  to:
    kind: Service
    name: "{{ _haproxy_name }}"
    weight: 100
  wildcardPolicy: None