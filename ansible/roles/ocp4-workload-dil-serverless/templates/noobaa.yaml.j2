apiVersion: noobaa.io/v1alpha1
kind: NooBaa
metadata:
  name: noobaa
  namespace: {{ ocs_project }}
  labels:
    app: noobaa
spec:
  cleanupPolicy:
    allowNoobaaDeletion: true
  dbType: postgres
  dbVolumeResources:
    requests:
      storage: 1Gi  
  endpoints:
    maxCount: 10
    minCount: 3  
  security:
    kms: {}
