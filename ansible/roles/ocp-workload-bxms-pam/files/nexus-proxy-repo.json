{
   "data": {
      "repoType": "proxy",
      "id": "{{ proxy_repo.name }}",
      "name": "{{ proxy_repo.name }}",
      "browseable": true,
      "indexable": true,
      "notFoundCacheTTL": 1440,
      "artifactMaxAge": -1,
      "metadataMaxAge": 1440,
      "itemMaxAge": 1440,
      "repoPolicy": "RELEASE",
      "provider": "maven2",
      "providerRole": "org.sonatype.nexus.proxy.repository.Repository",
      "downloadRemoteIndexes": false,
      "autoBlockActive": true,
      "fileTypeValidation": true,
      "exposed": true,
      "checksumPolicy": "WARN",
      "remoteStorage": {
         "remoteStorageUrl": "{{ proxy_repo.url }}",
         "authentication": null,
         "connectionSettings": null
      }
   }
}