apiVersion: image.openshift.io/v1
kind: ImageStream
metadata:
  # This a hack until we can include additional notebooks in the singleuser profile
  name: "{{ ocp4_workload_ai_ml_workflows_demo_workshop_jupyter_notebook_imagestream_name }}"
  namespace: "{{ namespace }}"
  labels:
    opendatahub.io/notebook-image: 'true'
spec:
  lookupPolicy:
    local: false
  tags:
    - from:
        kind: DockerImage
        name: "{{ ocp4_workload_ai_ml_workflows_demo_workshop_jupyter_notebook_imagestream_image }}"
      name: "{{ ocp4_workload_ai_ml_workflows_demo_workshop_jupyter_notebook_imagestream_tag }}"
      referencePolicy:
        type: Source
