# This file is generated by Ansible, do not edit manually.

[windows]
{% for host in groups['windows'] %}
{{ host.split('.')[0] }}.example.com ansible_host={{ hostvars[host]['ip_addresses_list']|trim }}
{% endfor %}

[bastions]
{% for host in groups['bastions'] %}
{{ host.split('.')[0] }}.idm.example.com ansible_host={{ hostvars[host]['ip_addresses_list']|trim }}
{% endfor %}

[nodes]
{% for host in groups['nodes'] %}
{{ host.split('.')[0] }}.idm.example.com ansible_host={{ hostvars[host]['ip_addresses_list']|trim }}
{% endfor %}

{% if groups['windows'] %}
[windows:vars]
ansible_connection=winrm
ansible_user=Administrator
ansible_become=false
ansible_password={{ common_password }}
ansible_winrm_server_cert_validation=ignore
ansible_port=5986
{% endif %}
