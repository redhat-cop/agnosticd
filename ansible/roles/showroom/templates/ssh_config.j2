{% for user in showroom_lab_users %}
# Entry for connecting as {{ user }}
Host {{ user }}
  HostName {{ showroom_host | default('bastion') }}
  User {{ user }}
  IdentityFile {{ showroom_user_home_dir }}/.ssh/id_ed25519
  Port 22
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  LogLevel ERROR

{% endfor %}
