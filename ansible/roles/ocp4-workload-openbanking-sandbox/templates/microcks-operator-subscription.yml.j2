---
apiVersion: operators.coreos.com/v1alpha1
kind: Subscription
metadata:
  name: microcks-operator
  namespace: {{ obsandbox_microcks_namespace }}
spec:
  installPlanApproval: Automatic
  channel: {{ obsandbox_microcks_operator_channel }}
  name: {{ obsandbox_microcks_operator_name }}
  source: {{ obsandbox_microcks_operator_source }}
  sourceNamespace: {{ obsandbox_microcks_operator_sourceNamespace }}
  startingCSV: {{ obsandbox_microcks_operator_startingCSV }}