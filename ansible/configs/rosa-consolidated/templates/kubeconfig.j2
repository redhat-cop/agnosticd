---
apiVersion: v1
kind: Config

clusters:
- cluster:
    insecure-skip-tls-verify: true
    server: {{ r_kube_auth.k8s_auth.host }}
  name: rosa

contexts:
- context:
    cluster: rosa
    namespace: default
    user: {{ r_kube_auth.k8s_auth.username }}
  name: admin

current-context: admin

users:
- name: {{ r_kube_auth.k8s_auth.username }}
  user:
    token: {{ r_kube_auth.k8s_auth.api_key }}
