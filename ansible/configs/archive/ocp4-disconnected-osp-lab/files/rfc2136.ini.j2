# Target DNS server
dns_rfc2136_server = {{ osp_cluster_dns_server }}
# Target DNS port
dns_rfc2136_port = 53
# TSIG key name
dns_rfc2136_name = {{ ddns_key_name }}
# TSIG key secret
dns_rfc2136_secret = {{ ddns_key_secret }}
# TSIG key algorithm
dns_rfc2136_algorithm = {{ ddns_key_algorithm | d('hmac-md5') }}
