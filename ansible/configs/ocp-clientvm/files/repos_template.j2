# RHEL Repos

{% if rhel_repos is defined %}
{% for i in range(0,rhel_repos|length) %}
[{{ rhel_repos[i] }}]
name={{ rhel_repos[i] }}
baseurl={{own_repo_path}}/{{ rhel_repos[i] }}
enabled=1
gpgcheck=0

{% endfor %}
{% else %}
[rhel-7-server-rpms]
name=Red Hat Enterprise Linux 7
baseurl={{own_repo_path}}/rhel-7-server-rpms
enabled=1
gpgcheck=0
{% endif %}