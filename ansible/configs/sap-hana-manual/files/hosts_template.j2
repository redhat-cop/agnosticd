[hana]
{% for host in groups['hanas'] %}
hana{{ loop.index }} ansible_host=hana{{ loop.index }}.{{ chomped_zone_internal_dns }}
{% endfor %}



[hana:children]
hanas

[hana:vars]
timeout=60
ansible_become=yes
ansible_user={{remote_user}}
ansible_ssh_private_key_file="~/.ssh/{{guid}}key.pem"
ansible_ssh_common_args="-o StrictHostKeyChecking=no"
