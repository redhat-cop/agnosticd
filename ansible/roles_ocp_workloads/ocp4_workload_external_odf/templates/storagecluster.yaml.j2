---
apiVersion: ocs.openshift.io/v1
kind: StorageCluster
metadata:
  name: ocs-external-storagecluster
  namespace: openshift-storage
spec:
  encryption: {}
  externalStorage:
    enable: true
  labelSelector: {}
  managedResources:
    cephBlockPools:
      disableStorageClass: true
    cephFilesystems: {}
    cephObjectStoreUsers: {}
    cephObjectStores: {}
