apiVersion: v1
kind: ConfigMap
metadata:
  name: workspace-userdata-gitconfig-configmap
  namespace: {{ devspace_user }}-devspaces
  labels:
    controller.devfile.io/mount-to-devworkspace: 'true'
    controller.devfile.io/watch-configmap: 'true'
  annotations:
    controller.devfile.io/mount-as: subpath
    controller.devfile.io/mount-path: /etc/
data:
  gitconfig: "[user]\n\tname = {{ ocp4_workload_redhat_developer_hub_backstage_gitlab_user_username }}\n\temail = {{ ocp4_workload_redhat_developer_hub_backstage_gitlab_user_email }}"
binaryData: {}
