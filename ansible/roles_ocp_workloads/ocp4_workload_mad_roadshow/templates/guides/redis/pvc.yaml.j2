---
apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: "{{ ocp4_workload_mad_roadshow_guides_user_selector_redis_name }}"
  namespace: "{{ ocp4_workload_mad_roadshow_guides_user_selector_redis_namespace }}"
spec:
  accessModes:
  - ReadWriteOnce
  storageClassName: "{{ ocp4_workload_mad_roadshow_storage_class_name }}"
  resources:
    requests:
      storage: "{{ ocp4_workload_mad_roadshow_guides_user_selector_redis_pvc_size }}"
