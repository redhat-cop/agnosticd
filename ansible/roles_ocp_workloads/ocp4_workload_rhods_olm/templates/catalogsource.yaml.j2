apiVersion: operators.coreos.com/v1alpha1
kind: CatalogSource
metadata:
  name: "{{ ocp4_workload_rhods_olm_catalogsource_name }}"
  namespace: "{{ ocp4_workload_rhods_olm_catalogsource_namespace }}"
spec:
  displayName: "{{ ocp4_workload_rhods_olm_catalogsource_display_name }}"
  image: "{{ ocp4_workload_rhods_olm_catalog_snapshot_image }}:{{ ocp4_workload_rhods_olm_catalog_snapshot_image_tag }}"
  sourceType: grpc
  publisher: OSD Red Hat Addons
  secrets:
    - addon-managed-odh-pullsecret