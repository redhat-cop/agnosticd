---
apiVersion: operators.coreos.com/v1alpha1
kind: Subscription
metadata:
  name: {{ starburst_sub_metadata_name }}
  namespace: {{ starburst_sub_metadata_namespace }}
spec:
  channel: {{ starburst_sub_spec_channel }}
  installPlanApproval: {{ starburst_sub_spec_install_plan }}
  name: {{ starburst_sub_spec_name }}
  source: {{ starburst_sub_spec_source }}
  sourceNamespace: {{ starburst_sub_spec_source_namespace }}
  startingCSV: {{ starburst_sub_spec_starting_csv }}