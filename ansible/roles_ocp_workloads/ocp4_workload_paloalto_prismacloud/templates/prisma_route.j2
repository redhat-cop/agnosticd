kind: Route
apiVersion: route.openshift.io/v1
metadata:
  name: twistlock-console
  namespace: {{ ocp4_workload_prisma_project }}
  labels:
    name: console
spec:
  to:
    kind: Service
    name: twistlock-console
  port:
    targetPort: management-port-https
  tls:
    termination: passthrough
    insecureEdgeTerminationPolicy: Redirect