    labels:
      app: cart
      demo: coolstore-microservice
    name: cart
    namespace: "{{ ocp4_workload_coolstore_apac_summit_coolstore_namespace }}"
  spec:
    failedBuildsHistoryLimit: 5
    nodeSelector: null
    output:
      to:
        kind: ImageStreamTag
        name: cart:latest
    resources:
      limits:
        cpu: 500m
        memory: 4Gi
      requests:
        cpu: 200m
        memory: 1Gi
    runPolicy: Serial
    source:
      contextDir: cart-service
      git:
        ref: main
        uri: {{ _ocp4_workload_coolstore_apac_summit_repo_url }}
      type: Git
    strategy:
      sourceStrategy:
        env:
        - name: MAVEN_MIRROR_URL
        from:
          kind: ImageStreamTag
          name: redhat-openjdk18-openshift:1.8
          namespace: openshift
      type: Source
    triggers:
    - type: ConfigChange
    - imageChange: {}
      type: ImageChange
    successfulBuildsHistoryLimit: 5
