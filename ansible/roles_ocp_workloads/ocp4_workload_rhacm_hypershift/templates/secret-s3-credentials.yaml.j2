---
apiVersion: v1
kind: Secret
metadata:
  name: hypershift-operator-oidc-provider-s3-credentials
  namespace: local-cluster
  labels:
    cluster.open-cluster-management.io/backup: "true"
stringData:
  bucket: {{ ocp4_workload_rhacm_hypershift_s3_bucket_name }}
  region: {{ ocp4_workload_rhacm_hypershift_s3_bucket_region }}
  credentials: |-
    [default]
    aws_access_key_id = {{ aws_access_key_id }}
    aws_secret_access_key = {{ aws_secret_access_key }}
