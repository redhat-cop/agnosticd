apiVersion: operators.coreos.com/v1
kind: OperatorGroup
metadata:
  name: my-group
  namespace: openshift-storage
spec:
  targetNamespaces:
  - openshift-storage
---
apiVersion: operators.coreos.com/v1alpha1
kind: Subscription
metadata:
  name: odf-operator
  namespace: openshift-storage
spec:
  channel: "{{ ocp4_workload_serverless_ml_workshop_odf_channel }}"
  installPlanApproval: Manual
  name: odf-operator
  source: "{{ ocp4_workload_serverless_ml_workshop_catalogsource_name }}"
  sourceNamespace: openshift-storage
  startingCSV: "{{ ocp4_workload_serverless_ml_workshop_odf_starting_csv }}"
