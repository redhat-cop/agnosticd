apiVersion: operators.coreos.com/v1
kind: OperatorGroup
metadata:
  name: my-group
  namespace: openshift-workspaces
spec:
  targetNamespaces:
  - openshift-workspaces
---
apiVersion: operators.coreos.com/v1alpha1
kind: Subscription
metadata:
  name: codeready-workspaces
  namespace: openshift-workspaces
spec:
  channel: "{{ ocp4_workload_serverless_ml_workshop_crw_channel }}"
  installPlanApproval: Manual
  name: codeready-workspaces
  source: "{{ ocp4_workload_serverless_ml_workshop_catalogsource_name }}"
  sourceNamespace: openshift-workspaces
  startingCSV: "{{ ocp4_workload_serverless_ml_workshop_crw_starting_csv }}"
