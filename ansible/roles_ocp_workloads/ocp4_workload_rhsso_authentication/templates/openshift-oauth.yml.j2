---
apiVersion: config.openshift.io/v1
kind: OAuth
metadata:
  name: cluster
spec:
  identityProviders:
  - name: rhsso
    type: OpenID
    mappingMethod: claim
    openID:
      claims:
        email:
        - email
        name:
        - name
        preferredUsername:
        - preferred_username
      clientID: idp-4-ocp
      clientSecret:
        name: openid-client-secret-bb6zw
      extraScopes: []
      issuer: >-
        https://{{ ocp4_workload_rhsso_authentication_rhsso_host }}/auth/realms/openshift
