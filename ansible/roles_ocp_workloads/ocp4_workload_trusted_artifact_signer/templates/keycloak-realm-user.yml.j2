apiVersion: keycloak.org/v1alpha1
kind: KeycloakUser
metadata:
  name: {{ ocp4_workload_trusted_artifact_signer_realm_user_basename }}{{ item + 1 }}
  labels:
    app: sso
spec:
  realmSelector:
    matchLabels:
      app: sso
  user:
    credentials:
      - temporary: false
        type: password
        value: {{ ocp4_workload_trusted_artifact_signer_realm_user_password }}
    username: user{{ item + 1 }}
    email: user{{ item + 1 }}@opentlc.com
    enabled: true
    emailVerified: true
