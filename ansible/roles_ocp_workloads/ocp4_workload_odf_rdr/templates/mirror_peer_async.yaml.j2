apiVersion: multicluster.odf.openshift.io/v1alpha1
kind: MirrorPeer
metadata:
  name: mirrorpeer-sync-5m
spec:
  items:
    - storageClusterRef:
        name: ocs-storagecluster
        namespace: openshift-storage
      clusterName: primary
    - storageClusterRef:
        name: ocs-storagecluster
        namespace: openshift-storage
      clusterName: secondary
  manageS3: true
  schedulingIntervals:
    - 5m
  type: async