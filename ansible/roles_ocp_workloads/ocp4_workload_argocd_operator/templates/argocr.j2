apiVersion: argoproj.io/v1alpha1
kind: ArgoCD
metadata:
  name: argocd
  namespace: "{{ ocp4_workload_argocd_operator_project }}"
spec:
  server:
    route: true
    ingress: false
    insecure: false
    resources: {}
  dex:
    image: quay.io/ablock/dex
    openShiftOAuth: true
    version: openshift-connector
  rbac:
    defaultPolicy: role:admin