kind: RoleBinding
apiVersion: rbac.authorization.k8s.io/v1
metadata:
  name: webhook-access-unauthenticated
  namespace: {{ ocp4_workload_jenkins_project }}
subjects:
  - kind: Group
    apiGroup: rbac.authorization.k8s.io
    name: 'system:unauthenticated'
roleRef:
  apiGroup: rbac.authorization.k8s.io
  kind: ClusterRole
  name: 'system:webhook'
