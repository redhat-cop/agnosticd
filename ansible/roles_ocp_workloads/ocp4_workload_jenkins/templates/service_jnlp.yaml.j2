apiVersion: v1
kind: Service
metadata:
  name: "{{ ocp4_workload_jenkins_name }}"
  namespace: "{{ ocp4_workload_jenkins_project }}"
spec:
  ports:
  - name: web
    nodePort: 0
    port: 80
    protocol: TCP
    targetPort: 8080
  selector:
    deployment: "{{ ocp4_workload_jenkins_name }}"
  sessionAffinity: None
  type: ClusterIP
