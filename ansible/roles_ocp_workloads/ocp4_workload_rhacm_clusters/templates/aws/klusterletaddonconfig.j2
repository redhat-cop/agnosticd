apiVersion: agent.open-cluster-management.io/v1
kind: KlusterletAddonConfig
metadata:
  name: {{ _ocp4_workload_rhacm_clusters_aws_cluster_name }}
  namespace: {{ _ocp4_workload_rhacm_clusters_aws_cluster_name }}
spec:
  clusterName: {{ _ocp4_workload_rhacm_clusters_aws_cluster_name }}
  clusterNamespace: {{ _ocp4_workload_rhacm_clusters_aws_cluster_name }}
  clusterLabels:
    cloud: Amazon
    vendor: OpenShift
  applicationManager:
    enabled: true
  policyController:
    enabled: true
  searchCollector:
    enabled: true
  certPolicyController:
    enabled: true
  iamPolicyController:
    enabled: true
