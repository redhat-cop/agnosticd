---
apiVersion: v1
kind: Service
metadata:
  name: "{{ ocp4_workload_codeserver_name }}"
  namespace: "{{ _ocp4_workload_codeserver_namespace }}"
  labels:
    app: "{{ ocp4_workload_codeserver_name }}"
spec:
  ports:
  - name: "{{ ocp4_workload_codeserver_name }}"
    port: 8080
    protocol: TCP
    targetPort: 8080
  selector:
    app: "{{ ocp4_workload_codeserver_name }}"
  type: ClusterIP
