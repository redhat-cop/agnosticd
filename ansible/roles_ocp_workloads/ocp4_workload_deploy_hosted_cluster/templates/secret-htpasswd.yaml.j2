---
apiVersion: v1
kind: Secret
metadata:
  name: htpasswd-{{ guid }}
  namespace: local-cluster
  labels:
    api.openshift.com/name: {{ ocp4_workload_deploy_hosted_cluster_name }}
    api.openshift.com/id: {{ ocp4_workload_deploy_hosted_cluster_infra_id }}
    api.openshift.com/type: identity-provider
type: Opaque
data:
  htpasswd: {{ r_htpasswd_file['content'] }}
