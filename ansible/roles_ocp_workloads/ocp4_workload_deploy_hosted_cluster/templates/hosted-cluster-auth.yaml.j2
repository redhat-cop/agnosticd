---
apiVersion: hypershift.openshift.io/v1beta1
kind: HostedCluster
metadata:
  name: {{ ocp4_workload_deploy_hosted_cluster_name }}
  namespace: local-cluster
spec:
  configuration:
    oauth:
      identityProviders:
      - name: democreds
        type: HTPasswd
        htpasswd:
          fileData:
            name: htpasswd-{{ guid }}
        mappingMethod: claim
      templates:
        error:
          name: ""
        login:
          name: ""
        providerSelection:
          name: ""
      tokenConfig: {}
