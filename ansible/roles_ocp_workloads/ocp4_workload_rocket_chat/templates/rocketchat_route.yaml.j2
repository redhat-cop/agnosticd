---
kind: Route
apiVersion: route.openshift.io/v1
metadata:
  name: rocket-chat
  namespace: "{{ ocp4_workload_rocket_chat_namespace }}"
spec:
  to:
    kind: Service
    name: rocket-chat
    weight: 100
  port:
    targetPort: 3000
  tls:
    termination: edge
    insecureEdgeTerminationPolicy: Redirect
  wildcardPolicy: None