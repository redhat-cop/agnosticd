---
apiVersion: build.openshift.io/v1
kind: BuildConfig
metadata:
  name: {{ item }}
  namespace: {{ t_project_name_test }}
spec:
  output:
    to:
      kind: ImageStreamTag
      name: {{ item }}:latest
  source:
    binary: {}
    type: Binary
  strategy:
    sourceStrategy:
      from:
        kind: ImageStreamTag
        name: ubi8-openjdk-11:1.3
        namespace: openshift
    type: Source
