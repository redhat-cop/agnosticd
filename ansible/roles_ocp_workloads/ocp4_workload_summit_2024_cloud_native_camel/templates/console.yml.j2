apiVersion: console.streamshub.github.com/v1alpha1
kind: Console
metadata:
  name: {{ user }}-console
spec:
  hostname: streams-console-{{ user }}-{{ globex_ns_suffix }}.{{ route_subdomain }}
  kafkaClusters:
    - name: kafka
      namespace: {{ user }}-{{ globex_ns_suffix }}
      listener: plain 
      properties:
        values: [] 
        valuesFrom: [] 
      credentials:
        kafkaUser:
          name: console-kafka-user