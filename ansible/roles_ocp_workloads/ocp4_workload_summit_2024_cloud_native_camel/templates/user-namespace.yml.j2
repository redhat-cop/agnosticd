{% for __user in users %}
---
apiVersion: v1
kind: Namespace
metadata:
  name: '{{ __user }}-{{ globex_ns_suffix }}'
---
apiVersion: v1
kind: Namespace
metadata:
  name: '{{ __user }}-{{ camel_ns_suffix }}'
---
apiVersion: v1
kind: Namespace
metadata:
  name: '{{ __user }}-devspaces'
  labels:
    app.kubernetes.io/part-of: che.eclipse.org
    app.kubernetes.io/component: workspaces-namespace
  annotations:
    che.eclipse.org/username: '{{ __user }}'

{% endfor %}    