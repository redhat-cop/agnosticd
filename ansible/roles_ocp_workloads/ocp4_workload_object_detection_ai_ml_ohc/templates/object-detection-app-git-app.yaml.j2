apiVersion: argoproj.io/v1alpha1
kind: Application
metadata:
  name: object-detection-app
  namespace: openshift-gitops
spec:
  destination:
    namespace: {{ ocp4_workload_aiml_namespace }}
    server: 'https://kubernetes.default.svc'
  project: default
  source:
    path: overlays/prod
    repoURL: >-
      {{ _ocp4_workload_object_detection_ai_ml_ohc_gitea_app_repo_url }}
    targetRevision: HEAD
  syncPolicy:
    automated: {}