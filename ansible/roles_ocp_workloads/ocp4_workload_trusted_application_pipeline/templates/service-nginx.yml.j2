kind: Service
apiVersion: v1
metadata:
  name: nginx
spec:
  ports:
    - name: 8080-tcp
      protocol: TCP
      port: 8080
      targetPort: 8080
    - name: 8443-tcp
      protocol: TCP
      port: 8443
      targetPort: 8443
    - name: 7777-tcp
      protocol: TCP
      port: 7777
      targetPort: 7777
  selector:
    app: nginx
