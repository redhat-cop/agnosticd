apiVersion: quota.openshift.io/v1
kind: ClusterResourceQuota
metadata:
  name: "clusterquota-shared-cluster-access-{{ guid }}"
spec:
  selector:
    annotations:
      openshift.io/requester: "{{ ocp_username }}"
    labels: null
  quota:
    hard:
      requests.cpu: "{{ ocp4_workload_shared_cluster_access_quota_requests_cpu }}"
      limits.cpu: "{{ ocp4_workload_shared_cluster_access_quota_limits_cpu }}"
      requests.memory: "{{ ocp4_workload_shared_cluster_access_quota_requests_memory }}"
      limits.memory: "{{ ocp4_workload_shared_cluster_access_quota_limits_memory }}"
      configmaps: "{{ ocp4_workload_shared_cluster_access_quota_configmaps }}"
      secrets: "{{ ocp4_workload_shared_cluster_access_quota_secrets }}"
      services: "{{ ocp4_workload_shared_cluster_access_quota_services }}"
      pods: "{{ ocp4_workload_shared_cluster_access_quota_pods }}"
      persistentvolumeclaims: "{{ ocp4_workload_shared_cluster_access_quota_persistentvolumeclaims }}"
      requests.storage: "{{ ocp4_workload_shared_cluster_access_quota_requests_storage }}"
