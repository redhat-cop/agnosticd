kind: Route
apiVersion: route.openshift.io/v1
metadata:
  name: vault
  namespace: "{{ ocp4_workload_hashicorp_vault_namespace }}"
spec:
  to:
    kind: Service
    name: vault
    weight: 100
  port:
    targetPort: http
  tls:
    termination: edge
    insecureEdgeTerminationPolicy: Redirect
  wildcardPolicy: None